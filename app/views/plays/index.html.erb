<h1>Listing plays</h1>

<table>
  <tr>
    <th>Start time</th>
    <th>Search blob</th>
    <th>Artist</th>
    <th>Song title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @plays.each do |play| %>
  <tr>
    <td><%= play.start_time %></td>
    <td><%= play.search_blob %></td>
    <td><%= play.artist %></td>
    <td><%= play.song_title %></td>
    <td><%= link_to 'Show', play %></td>
    <td><%= link_to 'Edit', edit_play_path(play) %></td>
    <td><%= link_to 'Destroy', play, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Play', new_play_path %>
